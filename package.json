{
  "name": "ultimate-trading-empire",
  "version": "1.0.0",
  "description": "Revolutionary Algorithmic Trading System - Scale from £3 to £1M+ with 31 Strategies",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:all": "npm run test:backtest && npm run test:paper && npm run test:integration && npm run test:production",
    "test:backtest": "ts-node src/tests/comprehensive-backtesting-engine.test.ts",
    "test:paper": "ts-node src/tests/paper-trading-validation-system.test.ts",
    "test:integration": "ts-node src/tests/run-integration-tests.ts",
    "test:production": "ts-node src/tests/production-deployment-test.ts",
    "test:ultimate": "ts-node src/tests/ultimate-trading-empire-test.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "deploy": "npm run build && npm run test:ultimate",
    "start:production": "NODE_ENV=production npm start"
  },
  "keywords": [
    "trading",
    "algorithmic-trading",
    "cryptocurrency",
    "arbitrage",
    "defi",
    "flash-loans",
    "whale-tracking",
    "automated-trading",
    "scaling",
    "unlimited-scaling"
  ],
  "author": "SuperKiro AI Trading Team",
  "license": "MIT",
