    "axios": "^1.6.0",
    "ccxt": "^4.1.0",
    "dotenv": "^16.3.0",
    "express": "^4.18.0",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.0",
    "moment": "^2.29.0",
    "node-cron": "^3.0.0",
    "openai": "^4.20.0",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.0",
